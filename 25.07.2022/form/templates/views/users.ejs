<%- include('./header')-%>
<div class="container">
<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First</th>
        <th scope="col">Last</th>
        <th scope="col">Handle</th>
      </tr>
    </thead>
    <tbody>
        <% users.forEach((element,index) => { %>
            <tr>
                <th scope="row"><%= index+1 %></th>
                <td><%= element.Name %></td>
                <td><%= element.Email %></td>
                <td><%= element.Phone %></td>
              </tr>
     <%   }); %>
    </tbody>
  </table>